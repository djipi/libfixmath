# Makefile defines for the libfixmath's M68K library


ACCURACY = -DFIXMATH_NO_ROUNDING -DFIXMATH_NO_OVERFLOW
PLATFORM = -DFIXMATH_NO_64BIT -DFIXMATH_NO_OPTIMIZE_8BIT -DFIXMATH_HARD_DIVISION
# FIXMATH_SATURATING_ARITHMETIC
COMPILE = -DFIXMATH_DEFINEVALUES -DFIXMATH_CTYPE -DFIXMATH_CACHE
ALGORITHMS = -DFIXMATH_SIN_LUT -DFIXMATH_FAST_SIN
